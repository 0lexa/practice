//JavaScript

const stringsCrossover = (inputArray, result) =>
  inputArray.reduce((pre, val, idx) => pre + inputArray.slice(idx + 1)
    .reduce((pre, v1) => pre + [...result].every((v2, idx) => v2 === val[idx] || v2 === v1[idx]), 0), 0);
