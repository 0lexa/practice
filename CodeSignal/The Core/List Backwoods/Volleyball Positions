//JavaScript

const volleyballPositions = (f, k) =>
  k % 6 > 0 ? volleyballPositions(f, --k, [f[2][1], f[3][0], f[1][0], f[0][1], f[1][2], f[3][2]] =
                                          [f[3][0], f[1][0], f[0][1], f[1][2], f[3][2], f[2][1]]) : f;
