//JavaScript

const drawRectangle = (canvas, r) => {
  canvas[r[1]][r[0]] = `*`;
  canvas[r[1]][r[2]] = `*`;
  canvas[r[3]][r[0]] = `*`;
  canvas[r[3]][r[2]] = `*`;
  for (let i = r[0] + 1; i < r[2]; i++) {
    canvas[r[1]][i] = `-`;
    canvas[r[3]][i] = `-`;
  }
  for (let j = r[1] + 1; j < r[3]; j++) {
    canvas[j][r[0]] = `|`;
    canvas[j][r[2]] = `|`;
  }
  return canvas
};
