//JavaScript

const comfortableNumbers = (l, r) => {
  let pairs = 0;
  for (let a = l; a < r; a++)
    for (let b = a + 1; b <= a + [...a.toString()].reduce((pre, val) => pre + +val, 0) && b <= r; b++)
      if (b - [...b.toString()].reduce((pre, val) => pre + +val, 0) <= a)
        pairs++;
  return pairs;
};
