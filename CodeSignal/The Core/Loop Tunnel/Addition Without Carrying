//JavaScript

const additionWithoutCarrying = (param1, param2) => {
  param1 = [...param1.toString().padStart(5)].map(val => +val);
  param2 = [...param2.toString().padStart(5)].map(val => +val);
  return +param1.map(val => (val + param2.shift()) % 10).join(``);
};
