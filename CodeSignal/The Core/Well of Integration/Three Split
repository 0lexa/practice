//JavaScript

const threeSplit = a => {
  const sum = a.reduce((pre, val) => pre + val, 0) / 3;
  let ways = 0;
  let sum1 = a[0];
  for (let i = 1; i < a.length; i++) {
    if (sum1 === sum) {
      let sum2 = a[i];
      for (let j = i + 1; j < a.length; j++) {
        if (sum2 === sum) ways += 1;
        sum2 += a[j];
      }
    }
    sum1 += a[i];
  }
  return ways;
};
