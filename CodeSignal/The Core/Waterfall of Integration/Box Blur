//JavaScript

const boxBlur = image =>
  image.slice(1, -1).map(val => val.slice(1, -1)).map((val, i) =>
    val.map((_, j) => [...Array(3).keys()].reduce((pre, x, _, a) =>
      pre + a.reduce((pre, y) =>
      pre + image[i + x][j + y], 0), 0) / 9 ^ 0));
