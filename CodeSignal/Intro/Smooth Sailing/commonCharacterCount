//JavaScript

function commonCharacterCount(s1, s2) {
  let tmp = s1.split(``),
    count = 0;
  while (tmp.length) {
    let char = tmp.shift();
    if (s2.includes(char)) {
      count++;
      s2 = s2.replace(char, ``);
    }
  }
  return count;
}
