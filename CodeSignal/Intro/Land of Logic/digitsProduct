//JavaScript

const digitsProduct = product => {
  if (product < 10) return product > 0 ? product : 10;
  for (let i = 25; i < product * 6; i++) {
    if ([...`${i}`].reduce((pre, val) => pre * +val, 1) === product) return i;
  }
  return -1;
};
