//JavaScript

const knapsackLight = (value1, weight1, value2, weight2, maxW) => {
  if (weight1 + weight2 <= maxW) return value1 + value2;
  if (weight2 <= maxW && value2 > value1) return value2;
  if (weight1 <= maxW) return value1;
  return 0;
};
