//JavaScript

const fizzBuzzCustom = (stringOne = `Fizz`, stringTwo = `Buzz`, numOne = 3, numTwo = 5) =>
  [...Array(100)].map((_, idx) => !(++idx % (numOne * numTwo)) ? stringOne + stringTwo : !(idx % numOne) ? stringOne : !(idx % numTwo) ? stringTwo : idx);
