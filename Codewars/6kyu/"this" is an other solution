//JavaScript

class OnceNamedOne {
  constructor(first, last) {
    Object.assign(this, {first, last});
  }
  
  get firstName() {
    return this.first;
  }
  
  get lastName() {
    return this.last;
  }
  
  get fullName() {
    return `${this.first} ${this.last}`;
  }
}
