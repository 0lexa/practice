//JavaScript

const calculateDamage = (yourType, opponentType, attack, defense) =>
  ((arr, obj) => 50 * attack / defense * arr[obj[yourType]][obj[opponentType]])
  ([[.5, .5, 2, 1], [2, .5, .5, .5], [.5, 2, .5, 1], [1, 2, 1, .5]], {fire: 0, water: 1, grass: 2, electric: 3});
