//JavaScript

Object.assign(Number.prototype,
  { add(n) {return (this + n)},
    sub(n) {return (this - n)},
    mul(n) {return (this * n)},
    div(n) {return (this / n)},
    pow(n) {return (this ** n)},
    sqr() {return (this ** .5)}
  });
