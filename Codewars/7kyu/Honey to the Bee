//JavaScript

class Bee {
  constructor(capacity, hive) {
    Object.assign(this, {capacity, hive});
  }

  unloadPollen() {
    this.hive.addPollen(this.capacity);
  }
}

class Hive {
  constructor() {
    this.pollen = 100;
  }

  addPollen(pollen) {
    this.pollen += pollen;
  }

  getPollen() {
    return this.pollen;
  }
}
