//JavaScript

const gymSlang = phrase =>
  phrase.replace(/(p)robably/gi, `$1rolly`)
    .replace(/(i) am/gi, `$1'm`)
    .replace(/(i)nstagram/gi, `$1nsta`)
    .replace(/(d)o not/gi, `$1on't`)
    .replace(/(g)oing to/gi, `$1onna`)
    .replace(/(c)ombination/gi, `$1ombo`);
