//JavaScript

const detectOperator = a =>
  /^8039/.test(a) ? `Golden Telecom` :
    /^80(50|66|95|99)/.test(a) ? `MTS` :
      /^80(63|93)/.test(a) ? `Life:)` :
        /^80(67|96|97|98)/.test(a) ? `Kyivstar` :
          /^8068/.test(a) ? `Beeline` : `no info`;
