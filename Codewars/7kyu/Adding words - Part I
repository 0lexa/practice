//JavaScript

class Arith {
  constructor(a) {
    this.a = a;
  }
  
  static get arr() {
    return [`zero`, `one`, `two`, `three`, `four`, `five`, `six`, `seven`, `eight`, `nine`, `ten`, `eleven`, `twelve`, `thirteen`, `fourteen`, `fifteen`, `sixteen`, `seventeen`, `eighteen`, `nineteen`, `twenty`];
  }
  
  add(b) {
    return Arith.arr[Arith.arr.indexOf(this.a) + Arith.arr.indexOf(b)];
  }
}
