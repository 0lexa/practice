const part = x =>
  (val => val ? `Mine's a Pint` + `!`.repeat(val) : `Lynn, I've pierced my foot on a spike!!`)
  (x.filter(val => [`Partridge`, `PearTree`, `Chat`, `Dan`, `Toblerone`, `Lynn`, `AlphaPapa`, `Nomad`].includes(val)).length);
