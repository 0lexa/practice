const expressionOut = expr =>
  ((a, op, b, nums) => OPERATORS[op] ? `${nums[a]} ${OPERATORS[op]}${nums[b]}` : `That's not an operator!`)
  (...expr.split(/\s/), [, `One`, `Two`, `Three`, `Four`, `Five`, `Six`, `Seven`, `Eight`, `Nine`, `Ten`]);
